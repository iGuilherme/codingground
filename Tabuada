#include <stdio.h>
int main(void)
{
    int i=2;
    int j=0;
    int tab;
    while(i<=10){
        printf("Tabuada do %d\n", i);
        while (j<=10){
            tab=i*j;
            printf("%d x %d = %d\n", i, j, tab);
            j++;
        }
    
        j=0;
        printf("_____________\n");
        i++;
    }
    return 0;
    
}
