#include <stdio.h>
int main(void)
{
    float nota1,nota2,nota3,nota4,MA;
    printf ("Digita sua nota 1:\n");
    scanf ("%f", &nota1);
printf ("Digita sua nota2:\n");
scanf ("%f", &nota2);
printf ("Digita sua nota3:\n");
scanf ("%f", &nota3);
printf ("Digita sua nota4:\n");
scanf ("%f", &nota4);
MA=((nota1+nota2+nota3+nota4)/4);
printf ("Sua media anual nessa materia foi %.1f", MA);
return 0;
}
